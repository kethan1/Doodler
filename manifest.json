{
    "manifest_version": 2,
    "name": "Doodler",
    "version": "0.0.2",
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["p5.min.js", "sketch.js"]
    }],
    "icons": {
        "16": "doodler16.png",
        "32": "doodler32.png",
        "48": "doodler48.png",
        "128": "doodler128.png"
    }
}